  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 8080",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "prepare": "xcopy electron dist /s /h /i /f /r",
    "electron": "npm run prepare && electron dist",
    "package": "npm run build && npm run prepare && electron-packager dist App --platform=win32 --arch=ia32 --out=app --win32metadata.CompanyName=\"Company\" --win32metadata.ProductName=\"App\" --overwrite && grunt create-windows-installer"
  },
